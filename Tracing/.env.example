# Azure AI Foundry Configuration
# Copy this file to .env and fill in your actual values

# Azure AI Foundry Endpoint (from your AI Foundry portal)
AZURE_AI_FOUNDRY_ENDPOINT_PORTAL=https://your-ai-foundry.services.ai.azure.com/api/projects/yourProject

# OpenAI API Version (recommended: 2024-02-15-preview)
AZURE_AI_FOUNDRY_OPENAI_API_VERSION=2024-02-15-preview

# Development/Debugging Options
# Set to "true" to enable console tracing output (useful for development)
ENABLE_CONSOLE_TRACING=true

# Optional: Other Azure AI configuration if needed for your setup
# AZURE_AI_FOUNDRY_OPENAI_KEY=your_openai_key
# AZURE_AI_FOUNDRY_OPENAI_DEPLOYMENT=your_deployment_name
# AZURE_AI_FOUNDRY_OPENAI_ENDPOINT=your_openai_endpoint
